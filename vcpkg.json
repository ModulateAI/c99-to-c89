{
  "name": "c99-to-c89",
  "version": "1.0.4",
  "description": "Tool to convert C99 code to MSVC-compatible C89. Includes fixes from Anaconda Distributions.",
  "homepage": "https://github.com/AnacondaRecipes/c99-to-c89/",
  "license": "Apache-2.0",
  "dependencies": [
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    },
    {
      "name": "llvm",
      "default-features": false,
      "features": [
        "clang"
      ]
    }
  ]
}
